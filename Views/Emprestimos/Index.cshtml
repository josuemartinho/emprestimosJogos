@model IEnumerable<EmprestimoModel>
<div>
    <div>
		<h1 class="">Emprestimos de Jogos</h1>
        <a class="btn btn-sucess">Novo Registro</a>
	</div>
</div>
<div class="text-center">

    <table class="table table-dark table-hover">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Recebedor</th>
                <th scope="col">Fornecedor</th>
                <th scope="col">Nome do Jogo</th>
                <th scope="col">Data de Atualização</th>
            </tr>
        </thead>
        <tbody>
            @if(Model!=null)
            {
                foreach(var dados in Model)
                {
            <tr>
                <th scope="row">@dados.Id</th>
                <td>@dados.Recebedor</td>
                <td>@dados.Fornecedor</td>
                <td>@dados.JogoEmprestado</td>
                <td>@dados.DataUltimaAtualizacao</td>
                </tr>
                }
            }
        </tbody>
    </table>
</div>
